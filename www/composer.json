{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "easycorp/easyadmin-bundle": "2.3.5",
        "sensio/framework-extra-bundle": "5.5.3",
        "symfony/console": "5.0.3",
        "symfony/flex": "^1.6",
        "symfony/form": "5.0.3",
        "symfony/framework-bundle": "5.0.3",
        "symfony/google-mailer": "5.0.3",
        "symfony/mailer": "5.0.3",
        "symfony/orm-pack": "1.0.7",
        "symfony/security-bundle": "5.0.3",
        "symfony/twig-pack": "^1.0",
        "symfony/webpack-encore-bundle": "1.7.2",
        "symfony/yaml": "5.0.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "src/Admin",
            "Core\\": "src/Core",
            "FrontOffice\\": "src/FrontOffice"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fixtures\\": "fixtures",
            "Tests\\": "tests"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "yarn",
            "yarn dev"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.3"
        }
    },
    "require-dev": {
        "brianium/habitat": "^1.0",
        "brianium/paratest": "3.1.*",
        "doctrine/doctrine-fixtures-bundle": "3.3.0",
        "nelmio/alice": "^3.5",
        "symfony/browser-kit": "5.0.3",
        "symfony/css-selector": "5.0.3",
        "symfony/phpunit-bridge": "5.0.3",
        "symfony/profiler-pack": "^1.0"
    }
}
